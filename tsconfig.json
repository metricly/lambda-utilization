{
  "version": "2.3.3",
  "compilerOptions": {
    "target": "es6",
    "mapRoot": "./js",
    "sourceMap": true,
    "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "declaration": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": "./js",
    "resolveJsonModule": true
  },
  "include": [
    "./ts/**/*.ts"
  ]
}
